<div class="navbar3_menu-background"></div>
<div class="navbar_container">
  <div class="navbar3_menu-button w-nav-button">
    <div class="menu-icon3">
      <div class="menu-icon3_line-top"></div>
      <div class="menu-icon3_line-middle"></div>
      <div class="menu-icon3_line-bottom"></div>
    </div>
  </div>
  <a
    href="{{ routes.root_url }}"
    id="w-node-_3038ef23-23d7-da69-89ae-5f8a1c720360-1c720358"
    class="navbar_logo_link w-nav-brand"><img
      src="{{ 'dreamfully-logo.svg' | asset_url }}"
      loading="lazy"
      alt=""
      class="navbar_logo"></a>
  <nav role="navigation" class="navbar-link-container w-nav-menu">
    {%- if section.settings.enable_quick_shopping -%}
      <div class="mobile-quick-shop-block">
        <div class="mobile-nav-heading quick">{{ section.settings.quick-shop-title }}</div>
        {%- assign product = section.settings.quick-product -%}
        <div class="quick-shop-block">
          <div class="text-block">
            {% if section.settings.product-title != blank %}
              {{ section.settings.product-title }}
            {% else %}
              {{ product.title }}{% endif %}
          </div>
          <div class="quick-shop-btn">
            <a href="{{section.settings.product-button-link}}" style="color: white; text-decoration: none;">{{ section.settings.product-button-text }}</a>
          </div>
          <div class="quick-shopimgwrap"><img
              src=" {% if section.settings.product-image != blank %} {{ section.settings.product-image | img_url: 'master' }} {% else %} {{ 'meddy-bear_1.png' | asset_url }} {% endif %}"
              loading="lazy"
              alt=""
              class="quick-shop-product-img"></div>
        </div>
      </div>
    {%- endif -%}

    {% for link in linklists[section.settings.main-menu_desktop].links %}
      <a href="{{link.url}}" class="navbar_link w-nav-link">{{ link.title }}</a>
    {% endfor %}

    <a href="#" class="navbar3_tablet-menu-button w-button">{{ 't77471352' | t }}</a>
    <div class="mobile-navigation">
      <div class="mobile-nav-heading">{{ section.settings.navigation-heading }}</div>
      {% for link in linklists[section.settings.main-menu_mobile].links %}
        <a href="{{link.url}}" class="mobile-link w-inline-block">{{ link.title }}</a>
      {% endfor %}
    </div>
    <div class="trustpilot-menu">
      <div class="_12-px-right">{{ section.settings.header-tp_text }}</div><img
        src="{{ section.settings.header-tp_svg }}"
        loading="lazy"
        alt=""
        class="trustpilot">
    </div>
  </nav>

  <div
    data-ajax-cart-toggle-class-button="js-my-cart-open"
    id="w-node-_3038ef23-23d7-da69-89ae-5f8a1c720380-1c720358"
    class="cart-btn animation-start"
    style="cursor: pointer;"><img
      src="{{ 'cart.svg' | asset_url }}"
      loading="lazy"
      alt="">
    <div class="active-cart-itms" data-ajax-cart-bind-state="cart.item_count">{{ cart.item_count }}</div>
  </div>
</div>


<script>
  var currentSection = document.currentScript.closest('.shopify-section');
  currentSection.setAttribute("data-animation", "over-left");
  currentSection.setAttribute("class", "navbar_component w-nav");
  currentSection.setAttribute("data-easing2", "ease");
  currentSection.setAttribute("fs-scrolldisable-element", "smart-nav");
  currentSection.setAttribute("data-easing", "ease");
  currentSection.setAttribute("data-collapse", "medium");
  currentSection.setAttribute("data-w-id", "3038ef23-23d7-da69-89ae-5f8a1c720358");
  currentSection.setAttribute("role", "banner");
  currentSection.setAttribute("data-no-scroll", "1");
  currentSection.setAttribute("data-duration", "400");
</script>

{% schema %}
  {
    "name": "Header",
    "tag": "div",
    "class": "navbar_component w-nav",
    "settings": [
      {
        "type": "checkbox",
        "id": "enable_quick_shopping",
        "label": "Enable quick shopping?",
        "default": true
      },
      {
        "type": "link_list",
        "id": "main-menu_desktop",
        "label": "Main Menu Links"
      },
      {
        "type": "link_list",
        "id": "main-menu_mobile",
        "label": "Main Menu Mobile Links"
      },
      {
        "type": "text",
        "id": "quick-shop-title",
        "label": "Quick Shop Title"
      }, {
        "type": "product",
        "id": "quick-product",
        "label": "Quick Product"
      }, {
        "type": "text",
        "id": "product-title",
        "label": "Product title"
      }, {
        "type": "image_picker",
        "id": "product-image",
        "label": "Product Image"
      }, {
        "type": "text",
        "id": "product-button-text",
        "label": "Product button text"
      }, {
        "type": "url",
        "id": "product-button-link",
        "label": "Product button link"
      }, {
        "type": "text",
        "id": "navigation-heading",
        "label": "Navigation Heading"
      }, {
        "type": "text",
        "id": "header-tp_text",
        "label": "TrustPilot Text"
      }, {
        "type": "url",
        "id": "header-tp_svg",
        "label": "Truspilot SVG"
      }
    ]
  }
{% endschema %}