<div bind="ce2938a5-0e23-d31f-75f6-3d9c7dc230ee" class="quick-atc-section">
  <div class="page-padding">
    <div class="container_v2">
      <div class="quick-atc-inner-container">
        <div class="quick-atc-half">
          <div class="product-slider">
            <div id="Product-Images-Slider" class="swiper-container swiper-initialized swiper-horizontal swiper-pointer-events swiper-backface-hidden">
              <div id="swiper-wrapper" class="swiper-wrapper">
                <div class="swiper-slide"><img
                    src="images/Image_1.png"
                    loading="lazy"
                    sizes="(max-width: 479px) 30vw, (max-width: 991px) 31vw, 15vw"
                    srcset="images/Image_1-p-500.png 500w, images/Image_1-p-800.png 800w, images/Image_1-p-1080.png 1080w, images/Image_1.png 1222w"
                    alt=""
                    class="product-img-slide"></div>
                <div class="swiper-slide"><img
                    src="images/Image_1.png"
                    loading="lazy"
                    sizes="(max-width: 479px) 30vw, (max-width: 991px) 31vw, 15vw"
                    srcset="images/Image_1-p-500.png 500w, images/Image_1-p-800.png 800w, images/Image_1-p-1080.png 1080w, images/Image_1.png 1222w"
                    alt=""
                    class="product-img-slide"></div>
                <div class="swiper-slide"><img
                    src="images/Image_1.png"
                    loading="lazy"
                    sizes="(max-width: 479px) 30vw, (max-width: 991px) 31vw, 15vw"
                    srcset="images/Image_1-p-500.png 500w, images/Image_1-p-800.png 800w, images/Image_1-p-1080.png 1080w, images/Image_1.png 1222w"
                    alt=""
                    class="product-img-slide"></div>
              </div>
              <div class="product-swiper-nav">
                <div id="product-left-arrow" class="product-swiper-arrow"><img
                    src="images/ic20-arrow-right-1.svg"
                    loading="lazy"
                    alt=""
                    class="slider-arrow-icon"></div>
                <div class="swiper-pagination-pdp swiper-pagination-bullets swiper-pagination-horizontal">
                  <div class="swiper-pagaintion-bullet"></div>
                </div>
                <div id="product-right-arrow" class="product-swiper-arrow"><img
                    src="images/ic20-arrow-right.svg"
                    loading="lazy"
                    alt=""
                    class="slider-arrow-icon"></div>
              </div>
            </div>
          </div>
        </div>
        <div class="quick-atc-half">
          <div class="quick-atc-content">
            <div class="atc-h1">The Meddybear - The Meditation Bear For Kids.</div>
            <div class="reviews-flex"><img
                src="images/Stars.svg"
                loading="lazy"
                alt=""
                class="quick-atc-review-stars">
              <div class="text-block-4">160 Reviews</div>
            </div>
            <div class="description">Introducing the Meditation Bear - the perfect bedtime companion for kids. Help your child relax and unwind with guided meditation and breathing exercises.</div>
            <p class="quick-atc-description">Introducing the Meditation Bear - the perfect bedtime companion for kids. Help your child relax and unwind with guided meditation and breathing exercises.</p>
            <div class="quick-atc-area">
              <div class="quick-atc-price-wrapper">
                <div class="price-wrapper">
                  <div class="before-price">€149,95</div>
                  <div class="price-h1">€49,95</div>
                </div>
                <div class="save-tag">Save €100,00</div>
              </div>
              <div class="quick-atc-shipping">
                <div class="div-block-4"></div>
                <div>
                  <strong>Free Shipping</strong>
                  - Ordered before 22:00, Tomorrow in house.</div>
              </div>
              <div class="atc-zone-quick">
                <div bind="ce2938a5-0e23-d31f-75f6-3d9c7dc2311f" class="form-block w-form">
                  <form
                    id="email-form"
                    name="email-form"
                    data-name="Email Form"
                    method="post"
                    class="form"><input
                      type="submit"
                      value="Add To Cart - €49,95"
                      data-wait="Adding to cart..."
                      class="primary-button quick-atc w-button">
                    <div class="input-counter-component">
                      <div bind="ce2938a5-0e23-d31f-75f6-3d9c7dc23123" class="fs_inputcounter-1_embed w-embed w-script">
                        <!--  [Finsweet Attributes] Input Counter  -->
                        <script>
                          (() => {
                            var t = "https://cdn.jsdelivr.net/npm/@finsweet/attributes-inputcounter@1/inputcounter.js",
                              e = document.querySelector(`script[src="${t}"]`);
                            e || (e = document.createElement("script"), e.async =! 0, e.src = t, document.head.append(e));
                          })();
                        </script>
                      </div>
                      <div class="fs_inputcounter-1_wrapper">
                        <div
                          aria-controls="fs-inputcounter-1-input"
                          fs-inputcounter-element="decrement"
                          role="button"
                          aria-label="Decrement input value"
                          tabindex="0"
                          class="fs_inputcounter-1_button">
                          <div>-</div>
                        </div><input
                          type="text"
                          fs-inputcounter-element="input"
                          class="fs_inputcounter-1_input w-input"
                          fs-inputcounter-initial="0"
                          maxlength="256"
                          name="Counter"
                          data-name="Counter"
                          placeholder=""
                          id="fs-inputcounter-1-input"
                          required="">
                        <div
                          aria-controls="fs-inputcounter-1-input"
                          fs-inputcounter-element="increment"
                          role="button"
                          aria-label="Increment input value"
                          tabindex="0"
                          class="fs_inputcounter-1_button">
                          <div>+</div>
                        </div>
                      </div>
                    </div>
                  </form>
                  <div class="w-form-done">
                    <div>Thank you! Your submission has been received!</div>
                  </div>
                  <div class="w-form-fail">
                    <div>Oops! Something went wrong while submitting the form.</div>
                  </div>
                </div>
              </div>
              <div class="upsell-bar quick">
                <div class="atc-bar-first"><img
                    src="images/Image.png"
                    loading="lazy"
                    alt=""
                    class="upsell-img">
                  <div class="upsell-title-wrapper">
                    <div class="upsell-title">Add my meditation pillow to this order</div>
                    <div>€24,95</div>
                  </div>
                </div>
                <div class="upsell-atc-btn"><img
                    src="images/cart.svg"
                    loading="lazy"
                    alt=""
                    class="cart-icon"></div>
              </div>
              <div class="payment-methods quick"><img
                  src="images/Group-300.svg"
                  loading="lazy"
                  alt=""
                  class="payment-icon"><img
                  src="images/Group-319.svg"
                  loading="lazy"
                  alt=""
                  class="payment-icon"><img
                  src="images/Group-318.svg"
                  loading="lazy"
                  alt=""
                  class="payment-icon"><img
                  src="images/mastercard.svg"
                  loading="lazy"
                  alt=""
                  class="payment-icon"><img
                  src="images/Group-315.svg"
                  loading="lazy"
                  alt=""
                  class="payment-icon"><img
                  src="images/Group-314.svg"
                  loading="lazy"
                  alt=""
                  class="payment-icon"><img
                  src="images/Group-313.svg"
                  loading="lazy"
                  alt=""
                  class="payment-icon"><img
                  src="images/Group-310.svg"
                  loading="lazy"
                  alt=""
                  class="payment-icon"></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="waves reverse"><img
      src="{{'reversed-wave.svg' | asset_url}}"
      loading="lazy"
      alt=""
      class="desktop-img"><img
      src="{{'tweede-mob-wave.svg' | asset_url}}"
      loading="lazy"
      alt=""
      class="mobile-img"></div>
</div>


{% schema %}
  {
    "presets": [
      {
        "name": "Quick ATC"
      }
    ],
    "name": "Quick ATC",
    "tag": "div",
    "class": "quick-atc",
    "settings": [
      {
        "type": "product",
        "id": "main-product",
        "label": "Main Product"
      }, {
        "type": "product",
        "id": "product-upsell",
        "label": "Product Upsell"
      }
    ]
  }
{% endschema %}